import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    char ch = 'A';
    int n = 5;
    for (int i = 1; i <= n; i++)
      {
	for (int j = 1; j <= i; j++)
	  {
	    System.out.print (ch + " ");
			      ch++;
			      }
			      System.out.println ();
			      }

			      }
			      }
-----------------------------------------------------------------------------------------------------------------------------
OUTPUT:
A 
B C 
D E F 
G H I J 
K L M N O 
